# Environment Variables Example
# Copy this file to .env and fill in your actual API keys
# NEVER commit .env to version control

# IP Information Service
VITE_IPINFO_API_KEY=your_ipinfo_api_key_here

# Authentication Service (Passage by 1Password)
VITE_PASSAGE_APP_ID=your_passage_app_id_here

# AI Services (Add your keys here)
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_key_here
VITE_GOOGLE_AI_API_KEY=your_google_ai_key_here
VITE_HUGGINGFACE_API_KEY=your_huggingface_key_here
VITE_STABILITY_API_KEY=your_stability_key_here
VITE_ELEVENLABS_API_KEY=your_elevenlabs_key_here
VITE_REPLICATE_API_TOKEN=your_replicate_token_here

# External Services
VITE_GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_id_here
VITE_GITHUB_OAUTH_CLIENT_ID=your_github_oauth_id_here
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Infrastructure (Server-side only - not prefixed with VITE_)
MONGODB_URI=your_mongodb_uri_here
CLOUDFLARE_API_TOKEN=your_cloudflare_token_here
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Optional Services
VITE_TWILIO_ACCOUNT_SID=your_twilio_sid_here
VITE_TWILIO_AUTH_TOKEN=your_twilio_token_here
